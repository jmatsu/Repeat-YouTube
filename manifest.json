{
  "manifest_version": 2,

  "name": "RepeatableYouTube",
  "description": "This extension makes YouTube movies repeatable",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "Toggle repeat"
  },
  "permissions": [
    "activeTab"
  ]
}
